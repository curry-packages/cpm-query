  "category": [ "Programming" ],
  "license": "BSD-3-Clause",
  "licenseFile": "LICENSE",
  "dependencies": {
    "base"        : " >= 3.2.0,  < 4.0.0",
    "currypath"   : " >= 3.0.0,  < 4.0.0",
    "directory"   : " >= 3.0.0,  < 4.0.0",
    "execpath"    : " >= 3.0.0,  < 4.0.0",
    "filepath"    : " >= 3.0.0,  < 4.0.0",
    "flatcurry"   : " >= 4.0.0,  < 5.0.0",
    "io-extra"    : " >= 3.0.0,  < 4.0.0",
    "process"     : " >= 3.0.0,  < 4.0.0",
    "propertyfile": " >= 3.0.0,  < 4.0.0",
    "socket"      : " >= 3.0.0,  < 4.0.0"
  },
  "executable": {
    "name": "cpm-query",
